{
  "version": 1,
  "paths": {
    "/items": {
      "name": "/items",
      "lambdaFunction": "itemsLambda",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    },
    "/book/guest": {
      "name": "/book/guest",
      "lambdaFunction": "itemsAuthLambda",
      "permissions": {
        "setting": "open"
      }
    }
  }
}